{
  "private": true,
  "scripts": {
    "build": "npx browserify src/index.js -o dist/index.js -dv -t [ babelify --presets [ es2015 ]]",
    "watch": "npx mkdirp dist && npx watchify src/index.js -o dist/index.js -dv -t [ babelify --presets [ es2015 ]]",
    "prestart": "test -e src/settings.json || echo {} > src/settings.json",
    "start": "npx concurrently \"npm run watch\" \"npx live-server --watch=dist/index.js,index.html\"",
    "test": "npx jest"
  },
  "dependencies": {
    "@cycle-robot-drivers/run": "1.0.26",
    "@cycle-robot-drivers/screen": "1.1.7",
    "@cycle/dom": "^22.3.0",
    "@cycle/run": "^5.2.0",
    "@cycle/state": "^1.2.0",
    "@cycle/time": "^0.19.0",
    "@mjyc/voice-activity-detection": "0.0.3",
    "cycle-posenet-driver": "1.0.6",
    "ltljs": "0.0.1",
    "xstream": "^11.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "speclang": "../speclang"
  }
}

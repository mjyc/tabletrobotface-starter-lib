{
  "name": "tabletrobotface-starter-lib",
  "version": "0.0.0",
  "description": "A library for tablet robot face starter project",
  "scripts": {
    "build": "npx browserify src/index.js -t babelify --outfile dist/index.js -dv",
    "watch": "npx mkdirp dist && npx watchify src/index.js -t babelify --outfile dist/index.js -dv",
    "prestart": "test -e src/settings.json || echo {} > src/settings.json",
    "start": "npx concurrently \"npm run watch\" \"npx live-server --watch=dist/index.js,index.html\""
  },
  "dependencies": {
    "@cycle-robot-drivers/run": "1.0.25",
    "@cycle-robot-drivers/screen": "1.1.7",
    "@cycle/dom": "22.3.0",
    "@cycle/run": "5.2.0",
    "@cycle/state": "1.2.0",
    "@cycle/time": "0.19.0",
    "@mjyc/cycle-time-travel": "0.1.4",
    "@mjyc/voice-activity-detection": "1.0.0",
    "cycle-posenet-driver": "1.0.8",
    "xstream": "11.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0"
  }
}
